<Window x:Class="semana2.Registrar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:semana2"
        mc:Ignorable="d"
        Title="Registrar" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Center" Margin="0,110,0,0" TextWrapping="Wrap" Text="Nombre" VerticalAlignment="Top" Width="220"/>
        <TextBox HorizontalAlignment="Center" Margin="0,153,0,0" TextWrapping="Wrap" Text="Licencia" VerticalAlignment="Top" Width="220"/>
        <TextBox HorizontalAlignment="Center" TextWrapping="Wrap" Text="Transporte" VerticalAlignment="Center" Width="220"/>
        <Button Content="Guardar" HorizontalAlignment="Left" Margin="218,333,0,0" VerticalAlignment="Top" Width="150"/>
        <Button Content="Salir" HorizontalAlignment="Left" Margin="436,333,0,0" VerticalAlignment="Top" Width="150"/>

    </Grid>
</Window>
